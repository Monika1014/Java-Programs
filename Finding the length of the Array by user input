import java.util.*;
public class Main{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int size=sc.nextInt();
        int[] array=new int[size];
        for (int i=0;i<size;i++)
        {
            array[i]=sc.nextInt();
        }
        int ar=array.length;
        System.out.println("The length of array is "+ar+".");
    }
}
